_ACEOF

    withval="/usr/local/ssl"     # default

    if test "${with_sslinc+set}" = set; then

	withval="$with_sslinc"
	case "$withval" in
	yes|no)
	  { echo "$as_me:$LINENO: WARNING: --with-sslinc called without argument - will use default" >&5
echo "$as_me: WARNING: --with-sslinc called without argument - will use default" >&2;}
	  CFLAGS="-I/usr/local/ssl/include $CFLAGS"
	  LIBS="-lssl -lcrypto $LIBS"
	  LDFLAGS="=L/usr/local/ssl/lib $LDFLAGS"
	;;
        * )
          CFLAGS="-I${withval}/include $CFLAGS"
	  LIBS="-lssl -l crypto $LIBS"
  	  LDFLAGS="-L${withval}/lib $LDFLAGS"
	;;
	esac

    else

	CFLAGS="-I/usr/local/ssl/include $CFLAGS"
	LIBS="-lssl -lcrypto $LIBS"
	LDFLAGS="-L/usr/local/ssl/lib $LDFLAGS"

    fi

    if test ! -d ${withval}; then
      { { echo "$as_me:$LINENO: error: called with --with-ssl, but ssl base directory ${withval} does not exist or is not a directory. Aborting config" >&5
echo "$as_me: error: called with --with-ssl, but ssl base directory ${withval} does not exist or is not a directory. Aborting config" >&2;}
   { (exit 1); exit 1; }; }
    fi

    CFLAGS="-DHAVE_CRYPT_DECL $CFLAGS"  # Damn, SSLeay defines its own

    ;;
  *)
    echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
    ;;
  esac
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6

fi;


